{
  "name":"kiss_net",
  "version":"0.0.1",
  "log_path":"/log/packet/kiss_net",
  "main_log":{
    "level":"DEBUG",
    "verbose":false
  },
  "packet":{
    "path":"CWD",
    "file":"packets_uva.json"
  },
  "thread_enable":{
    "network": true,
    "user":true
  },
  "network":{
    "thread_name":"Network_Thread",
    "name":"net",
    "type":"TCP",
    "ip":"0.0.0.0",
    "port":8000,
    "retry_time":5.0,
    "timeout":0.01,
    "log":{
      "verbose":true
    }
  },
  "user":{
    "thread_name":"User_Thread",
    "name":"ui",
    "auto":1,
    "auto_delay":2.0,
    "log":{
      "verbose":false
    }
  }
}
